doctype html
html(lang='en')
    head
        meta(charset='utf-8')
        meta(name='viewport' content='width=device-width, initial-scale=1.0, shrink-to-fit=no')
        title Untitled
        link(rel='stylesheet' href='assets/bootstrap/css/bootstrap.min.css')
        link(rel='stylesheet' href='assets/fonts/ionicons.min.css')
        link(rel='stylesheet' href='assets/css/Footer-Basic.css')
        link(rel='stylesheet' href='assets/css/Navigation-Clean.css')
        link(rel='stylesheet' href='assets/css/styles.css')
    body
        nav.navbar.navbar-light.navbar-expand-lg.navigation-clean
            .container
                a.navbar-brand(href='#') Blogs Website
                button.navbar-toggler(data-bs-toggle='collapse' data-bs-target='#navcol-1')
                    span.visually-hidden Toggle navigation
                    span.navbar-toggler-icon
                #navcol-1.collapse.navbar-collapse
                    ul.navbar-nav.ms-auto
                        li.nav-item
                            a.nav-link.active(href='#') Home
                        li.nav-item
                            a.nav-link(href='#') Blogs
                        li.nav-item
                            a.nav-link(href='#') Login/Register
                        li.nav-item.dropdown
                            a.dropdown-toggle.nav-link(aria-expanded='false' data-bs-toggle='dropdown' href='#') Dropdown
                            .dropdown-menu
                                a.dropdown-item(href='#') Home
                                a.dropdown-item(href='#') Blogs
                                a.dropdown-item(href='#') Register
        block content
        footer.footer-basic
            .social
                a(href='#')
                    i.icon.ion-social-instagram
                a(href='#')
                    i.icon.ion-social-snapchat
                a(href='#')
                    i.icon.ion-social-twitter
                a(href='#')
                    i.icon.ion-social-facebook
            ul.list-inline
                li.list-inline-item
                    a(href='#') Home
                li.list-inline-item
                    a(href='#') Services
                li.list-inline-item
                    a(href='#') About
                li.list-inline-item
                    a(href='#') Terms
                li.list-inline-item
                    a(href='#') Privacy Policy
            p.copyright Company Name Â© 2021
        script(src='/assets/bootstrap/js/bootstrap.min.js')
